{
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "**/*.@(css|js|jpg|jpeg|png|gif|svg|ico|woff|woff2)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000"
        }
      ]
    }
  ],
  "rewrites": [
    { "source": "/", "destination": "/index.html" }
  ]
}
